<!DOCTYPE html>
<html lang="en">
<head>
  <%-include('../partials/head') %>
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <body class="container">
<header>
  <%-include('../partials/header') %>
</header>
<!-- <h3>SHOW ALL ALUMNI</h3> -->
<div class="text-right">
<a href="/addalumni" class="btn btn-primary">Add Alumni</a>
</div>
<% alumni.forEach(function(a){ %>
  <div class="card-columns">
    <div class="card">
      <div class="card-body">
        <h1><%= a.alumniName  %></h1>
        <h2><%= a.cohortName  %></h2>
        <h2><%= a.batchName  %></h2>
        <p><%= a.designation  %></p>
        <h4><%= a.company  %></h4>
        <a href="/alumni/<%= a._id %>">See More</a>
      </div>
    </div>
  </div>
  <% }) %> 

<footer>
  <%- include('../partials/footer') %>
</footer>

</body>
</html>